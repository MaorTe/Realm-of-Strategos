{
   "name": "matchmaking-service",
   "version": "1.0.0",
   "main": "src/index.ts",
   "scripts": {
      "build": "npm install && tsc",
      "start": "node src/index.ts",
      "dev": "npx nodemon --signal SIGINT --inspect=0.0.0.0:9229 --nolazy src/index.ts"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "description": "",
   "dependencies": {
      "@maorte/strategos-services-common-package": "^1.0.5",
      "express": "^4.21.2",
      "ioredis": "^5.4.2",
      "redis": "^4.7.0",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^5.0.1",
      "uuid": "^11.0.4",
      "winston": "^3.17.0",
      "winston-daily-rotate-file": "^5.0.0",
      "ws": "^8.18.0"
   },
   "devDependencies": {
      "@types/express": "^5.0.0",
      "@types/node": "^22.10.5",
      "@types/swagger-jsdoc": "^6.0.4",
      "@types/swagger-ui-express": "^4.1.7",
      "@types/ws": "^8.5.13",
      "dotenv": "^16.4.7",
      "nodemon": "^3.1.9",
      "ts-node": "^10.9.2",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.7.2"
   },
   "files": [
      "dist/*",
      "types/*"
   ],
   "nodemonConfig": {
      "watch": [
         "src/*.ts"
      ],
      "ext": "ts",
      "execMap": {
         "ts": "node --require ts-node/register -r tsconfig-paths/register"
      },
      "ignore": [
         ".git",
         "node_modules/**/node_modules"
      ]
   }
}
