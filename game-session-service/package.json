{
   "name": "game-session-service",
   "version": "1.0.0",
   "main": "src/index.ts",
   "scripts": {
      "build": "npm install && tsc",
      "start": "node src/index.ts",
      "dev": "npx nodemon --signal SIGINT --inspect=0.0.0.0:9230 --nolazy src/index.ts",
      "test": "jest --passWithNoTests --config jest.config.ts"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "description": "",
   "dependencies": {
      "@maorte/strategos-services-common-package": "^1.0.27",
      "cors": "^2.8.5",
      "express": "^4.21.2",
      "ioredis": "^5.4.2",
      "jsonwebtoken": "^9.0.2",
      "kysely": "^0.27.5",
      "pg": "^8.13.1",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^5.0.1",
      "uuid": "^11.0.4",
      "winston": "^3.17.0",
      "winston-daily-rotate-file": "^5.0.0"
   },
   "devDependencies": {
      "@types/cors": "^2.8.17",
      "@types/express": "^5.0.0",
      "@types/jest": "^29.5.14",
      "@types/jsonwebtoken": "^9.0.7",
      "@types/node": "^22.10.5",
      "@types/pg": "^8.11.11",
      "@types/supertest": "^6.0.2",
      "@types/swagger-jsdoc": "^6.0.4",
      "@types/swagger-ui-express": "^4.1.7",
      "@types/uuid": "^10.0.0",
      "dotenv": "^16.4.7",
      "jest": "^29.7.0",
      "nodemon": "^3.1.9",
      "supertest": "^7.0.0",
      "ts-jest": "^29.2.5",
      "ts-node": "^10.9.2",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.7.2"
   },
   "files": [
      "dist/*",
      "types/*"
   ],
   "nodemonConfig": {
      "watch": [
         "src/*.ts"
      ],
      "ext": "ts",
      "execMap": {
         "ts": "node --require ts-node/register -r tsconfig-paths/register"
      },
      "ignore": [
         ".git",
         "node_modules/**/node_modules"
      ]
   }
}
